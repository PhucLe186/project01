    "ChiTietMonAn": {
        "ID_ChiTietMon_1": {
            "MoTa": "Bánh Tổ là món tráng miệng ngọt, thơm hương vani, vị bơ béo ngậy.",
            "NguyenLieuChinh": ["Bột gạo", "Đường", "Vani", "Bơ"],
            "CachCheBien": "Trộn bột gạo với đường, thêm vani, hấp trong khuôn rồi rán sơ cho vàng."
        },
        "ID_ChiTietMon_2": {
            "MoTa": "Lẩu Cừu thơm ngọt, nước dùng đậm đà, thịt cừu mềm ngon.",
            "NguyenLieuChinh": ["Thịt cừu", "Nấm", "Rau thơm", "Gia vị lẩu"],
            "CachCheBien": "Ninh xương cừu kỹ, thêm gia vị, rau thơm, thịt cừu cắt lát nhúng lẩu."
        },
        "ID_ChiTietMon_3": {
            "MoTa": "Mì Vịt Tiềm là món mì nước với vị thuốc Bắc, vị ngọt thanh của nước dùng.",
            "NguyenLieuChinh": ["Vịt", "Mì tươi", "Thuốc bắc", "Gia vị"],
            "CachCheBien": "Hầm vịt với thuốc bắc, nấu nước dùng, luộc mì, ăn kèm rau thơm."
        },
        "ID_ChiTietMon_4": {
            "MoTa": "Phật Nhảy Tường là món ăn đặc sản với nhiều nguyên liệu quý hiếm.",
            "NguyenLieuChinh": ["Hải sâm", "Vi cá", "Nấm đông cô", "Rượu nếp", "Thịt heo"],
            "CachCheBien": "Hầm các nguyên liệu trên lửa nhỏ, giữ hương vị thơm ngon, đậm đà."
        },
        "ID_ChiTietMon_5": {
            "MoTa": "Sủi Cảo là món bánh gói nhân thịt hoặc tôm, hấp hoặc chiên giòn.",
            "NguyenLieuChinh": ["Bột mì", "Thịt xay", "Tôm", "Gia vị"],
            "CachCheBien": "Trộn nhân, gói vỏ bánh, hấp hoặc chiên cho chín vàng."
        },
        "ID_ChiTietMon_6": {
            "MoTa": "Tôm Xào Hạt Điều là món tôm tươi xào cùng hạt điều béo ngậy.",
            "NguyenLieuChinh": ["Tôm tươi", "Hạt điều", "Tỏi", "Gia vị xào"],
            "CachCheBien": "Xào tôm với tỏi và hạt điều, nêm nếm gia vị vừa ăn."
        },
        "ID_ChiTietMon_7": {
            "MoTa": "Thịt Đông Pha là món thịt đông lạnh, thái mỏng, ăn kèm gia vị đặc trưng.",
            "NguyenLieuChinh": ["Thịt heo", "Gia vị chấm", "Rau sống"],
            "CachCheBien": "Nấu đông thịt, thái mỏng, ăn kèm rau và nước chấm."
        },
        "ID_ChiTietMon_8": {
            "MoTa": "Thịt Lợn Chua Ngọt là món thịt lợn xào sốt chua ngọt đậm đà.",
            "NguyenLieuChinh": ["Thịt lợn", "Dứa", "Ớt chuông", "Sốt chua ngọt"],
            "CachCheBien": "Xào thịt với dứa và ớt chuông, thêm sốt chua ngọt, đảo đều."
        },
        "ID_ChiTietMon_9": {
            "MoTa": "Vịt Quay Bắc Kinh da giòn, thịt mềm, vị đậm đà, ăn kèm bánh và rau.",
            "NguyenLieuChinh": ["Vịt", "Gia vị quay", "Bánh tráng", "Hành, dưa leo"],
            "CachCheBien": "Quay vịt cho da giòn, thái miếng, ăn cùng bánh tráng và rau."
        },
        "ID_ChiTietMon_10": {
            "MoTa": "Bún Qua Cầu là món bún nước với nhiều loại nguyên liệu phong phú.",
            "NguyenLieuChinh": ["Bún", "Thịt heo", "Trứng", "Gia vị nước dùng"],
            "CachCheBien": "Nấu nước dùng trong, chần bún, thêm thịt, trứng, rau."
        },
        "ID_ChiTietMon_11": {
            "MoTa": "Canh YanDuXian là món canh thơm ngon, thanh mát, giàu dinh dưỡng.",
            "NguyenLieuChinh": ["Thịt bò", "Rau cải", "Gia vị nêm"],
            "CachCheBien": "Nấu canh với thịt và rau, nêm gia vị vừa ăn."
        },
        "ID_ChiTietMon_12": {
            "MoTa": "Cơm Chiên Dương Châu là món cơm chiên thập cẩm với nhiều nguyên liệu.",
            "NguyenLieuChinh": ["Cơm", "Thịt xá xíu", "Tôm", "Đậu Hà Lan", "Trứng"],
            "CachCheBien": "Chiên cơm cùng thịt, tôm, đậu và trứng, nêm gia vị."
        },
        "ID_ChiTietMon_13": {
            "MoTa": "Chả Giò Quảng Đông là món chả giò giòn rụm, nhân thịt và rau củ.",
            "NguyenLieuChinh": ["Thịt heo", "Rau củ", "Vỏ bánh tráng"],
            "CachCheBien": "Cuộn nhân trong bánh tráng, chiên vàng giòn."
        },
        "ID_ChiTietMon_14": {
            "MoTa": "Chow Mein - Mì Xào là món mì xào giòn hoặc mềm cùng rau và thịt.",
            "NguyenLieuChinh": ["Mì trứng", "Thịt gà", "Rau củ", "Nước sốt xào"],
            "CachCheBien": "Xào mì với thịt và rau củ, nêm gia vị vừa ăn."
        },
        "ID_ChiTietMon_15": {
            "MoTa": "Đậu Hủ Tứ Xuyên là món đậu phụ cay đặc trưng vùng Tứ Xuyên.",
            "NguyenLieuChinh": ["Đậu phụ", "Ớt bột", "Gia vị cay"],
            "CachCheBien": "Xào đậu phụ với nước sốt cay, hành, tỏi."
        },
        "ID_ChiTietMon_16": {
            "MoTa": "Đậu Hủ Thối là món đậu hũ lên men có mùi đặc trưng, vị béo.",
            "NguyenLieuChinh": ["Đậu hũ lên men", "Gia vị"],
            "CachCheBien": "Chiên đậu hũ lên men, ăn kèm nước chấm đặc biệt."
        },
        "ID_ChiTietMon_17": {
            "MoTa": "Gà An Mây là món gà nướng thơm ngon, da giòn, thịt mềm.",
            "NguyenLieuChinh": ["Gà", "Gia vị ướp", "Gia vị nướng"],
            "CachCheBien": "Ướp gà, nướng trên than hoa hoặc lò nướng."
        },
        "ID_ChiTietMon_18": {
            "MoTa": "Gà Kung Pao Tứ Xuyên là món gà xào cay đặc trưng vùng Tứ Xuyên.",
            "NguyenLieuChinh": ["Gà", "Ớt", "Đậu phộng", "Gia vị cay"],
            "CachCheBien": "Xào gà với ớt, đậu phộng, hành tỏi, nước sốt cay."
        },
        "ID_ChiTietMon_19": {
            "MoTa": "Kẹo Hồ Lô là món kẹo truyền thống với lớp đường caramel giòn tan.",
            "NguyenLieuChinh": ["Đường", "Mạch nha"],
            "CachCheBien": "Nấu đường tạo caramel, quấn quanh que tạo hình."
        },
        "ID_ChiTietMon_20": {
            "MoTa": "Kẹo Lạc Làm Thủ Công là món kẹo truyền thống làm từ lạc rang và đường mật thơm ngon.",
            "NguyenLieuChinh": ["Lạc", "Đường mật"],
            "CachCheBien": "Trộn lạc rang với đường mật, ép khuôn và để nguội."
        },
        "ID_ChiTietMon_21": {
            "MoTa": "Mì Xào Hương Vị Biển là món mì xào kết hợp các loại hải sản tươi ngon và rau củ.",
            "NguyenLieuChinh": ["Mì", "Hải sản", "Rau củ", "Gia vị xào"],
